{
    "name": "github-runner",
    "cmd": "",
    "cpu_priority": "90",
    "enable_publish_all_ports": false,
    "enable_restart_policy": true,
    "env_variables": [
        {
            "key": "ACCESS_TOKEN",
            "value": "{{access_token}}"
        },
        {
            "key": "RUNNER_WORKDIR",
            "value": "{{SYNOPKG_PKGTMP}}"
        },
        {
            "key": "RUNNER_SCOPE",
            "value": "org"
        },
        {
            "key": "ORG_NAME",
            "value": "{{org_name}}"
        },
        {
            "key": "LABELS",
            "value": "{{labels}}"
        }
    ],
    "image": "myoung34/github-runner:2.325.0-ubuntu-noble",
    "is_package": true,
    "memory_limit": 0,
    "port_bindings": [],
    "privileged": true,
    "shortcut": {
        "enable_shortcut": false
    },
    "volume_bindings": [
        {
            "host_absolute_path": "{{DOCKER_SOCKET}}",
            "mount_point": "/var/run/docker.sock",
            "type": "rw"
        },
        {
            "host_absolute_path": "{{SYNOPKG_PKGTMP}}",
            "mount_point": "{{SYNOPKG_PKGTMP}}",
            "type": "rw"
        }
    ]
}
